				float3 rotatedValue94 = RotateAroundAxis( float3( 0,0,0 ), ase_worldPos, _Vector0.xyz, _Vector0.w );
				float gradientNoise91 = UnityGradientNoise(rotatedValue94.xy,_NoiseScale);
				gradientNoise91 = gradientNoise91*0.5 + 0.5;
				float _speed18 = _Speed;
				float mulTime20 = _TimeParameters.x * _speed18;
				float gradientNoise97 = UnityGradientNoise(( mulTime20 + rotatedValue94 ).xy,_NoiseScale);
				gradientNoise97 = gradientNoise97*0.5 + 0.5;
