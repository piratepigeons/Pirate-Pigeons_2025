				float gradientNoise109 = GradientNoise(( rotatedValue94 + ( mulTime20 * _Float1 ) ).xy,_BaseScale);
				gradientNoise109 = gradientNoise109*0.5 + 0.5;
				

				#ifdef ASE_ABSOLUTE_VERTEX_POS
					float3 defaultVertexValue = v.vertex.xyz;
				#else
					float3 defaultVertexValue = float3(0, 0, 0);
				#endif

				float3 vertexValue = ( v.vertex.xyz + ( ( v.ase_normal * ( ( smoothstepResult107 + ( gradientNoise109 * 2.0 ) ) / ( 1.0 + _Float2 ) ) ) * _Strength ) );

				#ifdef ASE_ABSOLUTE_VERTEX_POS
					v.vertex.xyz = vertexValue;
